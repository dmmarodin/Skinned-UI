{
	"name": "skinned-ui",
	"description": "Styleless UI library for Svelte",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dmmarodin/skinned-ui.git"
	},
	"author": "Douglas Marodin",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dmmarodin/skinned-ui/issues"
	},
	"homepage": "https://github.com/dmmarodin/skinned-ui#readme",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "vitest run src",
		"test:e2e": "playwright test",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"dependencies": {
		"@skinned-ui/style": "./src/lib/style"
	},
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@playwright/test": "^1.22.2",
		"@storybook/addon-actions": "^6.5.9",
		"@storybook/addon-essentials": "^6.5.9",
		"@storybook/addon-interactions": "^6.5.9",
		"@storybook/addon-links": "^6.5.9",
		"@storybook/addon-svelte-csf": "^2.0.6",
		"@storybook/builder-vite": "^0.2.2",
		"@storybook/svelte": "^6.5.9",
		"@storybook/testing-library": "^0.0.13",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/vite-plugin-svelte": "^1.0.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.0",
		"@types/autoprefixer": "^10.2.0",
		"@types/eslint": "^8.4.5",
		"@types/eslint-config-prettier": "^6.11.0",
		"@types/jsdom": "^20.0.0",
		"@types/prettier": "^2.6.3",
		"@types/sass": "^1.43.1",
		"@types/tailwindcss": "^3.1.0",
		"@types/typescript": "^2.0.0",
		"autoprefixer": "^10.4.7",
		"babel-loader": "^8.2.5",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^20.0.0",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.54.0",
		"storybook-dark-mode": "^1.1.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.7.1",
		"svelte-loader": "^3.1.3",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.12",
		"tailwindcss": "^3.1.6",
		"typescript": "^4.7.4",
		"vite": "^3.0.9",
		"vite-tsconfig-paths": "^3.5.0",
		"vitest": "^0.22.1"
	},
	"type": "module"
}
